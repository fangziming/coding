    <name>getCourseList</name>
    <function-class>com.thinkgem.jeesite.modules.zxy.utils.ZxyUtils</function-class>
    <function-signature>java.util.List getCourseList()</function-signature>
    <example>${zxy:getCourseList()}</example>  
  </function>
  
  <function>
    <description>获得课程信息</description>
    <name>getCourse</name>
    <function-class>com.thinkgem.jeesite.modules.zxy.utils.ZxyUtils</function-class>
    <function-signature>com.thinkgem.jeesite.modules.zxy.entity.Course getCourse(java.lang.String)</function-signature>
    <example>${zxy:getCourse(courseId)}</example>  
  </function>
  
  <function>
    <description>获取章节列表</description>
    <name>getUnitList</name>
    <function-class>com.thinkgem.jeesite.modules.zxy.utils.ZxyUtils</function-class>
