    <description>获取章节列表</description>
    <name>getUnitListByCourseId</name>
    <function-class>com.thinkgem.jeesite.modules.zxy.utils.ZxyUtils</function-class>
    <function-signature>java.util.List getUnitListByCourseId(java.lang.String)</function-signature>
    <example>${zxy:getUnitListByCourseId(courseId)}</example>  
  </function>
  
  <function>
    <description>获得章节信息</description>
    <name>getUnit</name>
    <function-class>com.thinkgem.jeesite.modules.zxy.utils.ZxyUtils</function-class>
